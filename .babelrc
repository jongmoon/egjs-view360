{
    "presets": [ 
        [
            "@babel/preset-env", 
            {
                "loose": true,
                "modules": false
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", {"loose": true}],
        "transform-es3-property-literals",
        "transform-es3-member-expression-literals",
        "transform-object-assign",
        "es6-promise"
    ]
}

